{
  "name": "@cf-eventhub/web-console",
  "type": "module",
  "version": "0.0.3",
  "scripts": {
    "dev": "wrangler dev",
    "deploy": "wrangler deploy --minify",
    "build": "tsup"
  },
  "dependencies": {
    "@hono/valibot-validator": "^0.5.1",
    "hono": "^4.6.10",
    "valibot": "^0.42.0"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20241022.0",
    "tsup": "^8.3.5",
    "typed-htmx": "^0.3.1",
    "typescript": "^5.6.2",
    "wrangler": "^3.83.0",
    "cf-eventhub": "^0.0.4"
  },
  "tsup": {
    "entry": ["src/index.ts"],
    "splitting": false,
    "sourcemap": true,
    "format": ["esm", "cjs"],
    "dts": true,
    "clean": true
  },
  "exports": {
    "import": "./dist/index.js",
    "require": "./dist/index.cjs"
  },
  "files": ["dist"]
}
